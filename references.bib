@article{Carlson2020WhaWou,
  title = {What Would It Take to Describe the Global Diversity of Parasites?},
  author = {Carlson, Colin J. and Dallas, Tad A. and Alexander, Laura W. and Phelan, Alexandra L. and Phillips, Anna J.},
