  title = {Metapopulation {{Models}} for {{Extinction Threshold}} in {{Spatially Correlated Landscapes}}},
  author = {Ovaskainen, OTSO and Sato, KAZUNORI and Bascompte, JORDI and Hanski, ILKKA},
