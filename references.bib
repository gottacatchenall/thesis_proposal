@article{Chase2003ComAss,
  title = {Community Assembly: When Should History Matter?},
  shorttitle = {Community Assembly},
  author = {Chase, Jonathan M.},
  year = {2003},
  month = aug,
  journal = {Oecologia},
  volume = {136},
  number = {4},
