@article{Franco2015ConNot,
  title = {To Connect or Not to Connect Isolated Patches},
  author = {Franco, Daniel and {Ruiz-Herrera}, Alfonso},
  year = {2015},
  month = apr,
  journal = {Journal of Theoretical Biology},
  volume = {370},
